        {
            "name": "Python: Experiment Default",
            "type": "python",
            "request": "launch",
            "module": "bin.local_search",
            "justMyCode": true,
            "args": [
                "--scenario",
                "examples/scenario/triangle-cpp_runtime.txt"
            ]
        },
        {
            "name": "Python: revalidate_patch",
            "type": "python",
            "request": "launch",
            "module": "bin.revalidate_patch",
            "justMyCode": true,
            "args": [
                "--scenario", 
                "examples/scenario/triangle-cpp_runtime.txt",
                "--patch",
                "StmtReplacement(('triangle.cpp.xml', 'stmt', 3), ('triangle.cpp.xml', 'stmt', 12))",
                "--output_dir",
                "test_dir"
            ]
        },
        {
            "name": "Python: Parse Results",
            "type": "python",
            "request": "launch",
            "module": "experiments.parse_results",
            "justMyCode": true,
            "args": [
                "--result_dir",
                "experiments/results"
            ]
        },
        {
            "name": "Python: Automate Run",
            "type": "python",
            "request": "launch",
            "program": "experiments/automate_run.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--step",
                "setup",
                "--results_dir",
